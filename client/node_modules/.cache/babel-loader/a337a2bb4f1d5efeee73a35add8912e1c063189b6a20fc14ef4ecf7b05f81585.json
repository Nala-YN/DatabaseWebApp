{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'RegisterView',\n  // const method = (rule, value, callback) => {\n\n  // },\n  data() {\n    return {\n      userid: '',\n      username: '',\n      password: '',\n      phoneNum: '',\n      email: '',\n      campus: '',\n      address: '',\n      confirmPassword: '',\n      step: 1\n    };\n  },\n  methods: {\n    nextPage() {\n      let emailReg = /^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)+$/;\n      let passwordReg = /^[.]{6,}$/;\n      if (this.username.length === 0 || this.password.length === 0 || this.email.length === 0 || this.confirmPassword.length === 0) {\n        ElMessage({\n          message: \"输入不可为空\",\n          type: 'warning'\n        });\n      } else if (!emailReg.test(this.email)) {\n        // 如果不符合，显示错误信息\n        alert('请输入正确的邮箱地址');\n      } else if (!passwordReg.test(this.password)) {\n        ElMessage({\n          message: \"密码长度需大于等于6\",\n          type: 'warning'\n        });\n      } else if (this.password !== this.confirmPassword) {\n        ElMessage({\n          message: \"二次密码输入不一致\",\n          type: 'warning'\n        });\n      } else {\n        this.step = 2;\n      }\n    },\n    submitForm() {\n      this.$refs.registerForm.validate(valid => {\n        if (valid) {\n          // 执行注册逻辑，例如向后端发送注册请求\n          // 在注册成功后，可以进行页面跳转或其他操作\n          // 示例：向控制台打印注册信息\n          console.log('注册信息:', this.registerForm);\n          this.$router.push({\n            name: 'Main'\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["ElMessage","name","data","userid","username","password","phoneNum","email","campus","address","confirmPassword","step","methods","nextPage","emailReg","passwordReg","length","message","type","test","alert","submitForm","$refs","registerForm","validate","valid","console","log","$router","push"],"sources":["E:\\Uni_homework\\DB\\test\\DatabaseWebApp\\client\\src\\components\\Register.vue"],"sourcesContent":["<template>\r\n  <!-- 整体背景 -->\r\n  <div id=\"body\" class=\"login-wrap\">\r\n    <div id=\"background\"></div>\r\n    <!--输入框-->\r\n    <div class=\"form-wrapper\">\r\n      <div class=\"header\">创建书觅新账户</div>\r\n      <div v-if=\"step === 1\" class=\"input-wrapper\">\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"text\" name=\"username\" placeholder=\"username\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"username\" />\r\n        </div>\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"email\" name=\"email\" placeholder=\"email\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"email\" />\r\n        </div>\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"password\" name=\"password\" placeholder=\"password\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"password\" />\r\n        </div>\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"password\" name=\"confirm\" placeholder=\"confirm\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"confirmPassword\" />\r\n        </div>\r\n      </div>\r\n      <div v-else class=\"input-wrapper\">\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"text\" name=\"phone\" placeholder=\"phone\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"phoneNum\" />\r\n        </div>\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"text\" name=\"campus\" placeholder=\"campus\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"campus\" />\r\n        </div>\r\n        <div class=\"border-wrapper\">\r\n          <input type=\"text\" name=\"address\" placeholder=\"address\" class=\"border-item\" autocomplete=\"off\"\r\n            v-model=\"address\" />\r\n        </div>\r\n      </div>\r\n\r\n      <div v-if=\"step === 1\" class=\"action\">\r\n        <el-button class=\"btn\" :plain=\"true\" @click=\"nextPage\">下一步</el-button>\r\n      </div>\r\n      <div v-else class=\"action\">\r\n        <el-button class=\"btn\" :plain=\"true\" @click=\"step = 1\">上一步</el-button>\r\n        <el-button class=\"btn\" :plain=\"true\" @click=\"submitForm\">注册</el-button>\r\n      </div>\r\n      <br />\r\n    </div>\r\n  </div>\r\n  \r\n</template>\r\n\r\n<script>\r\nimport { ElMessage } from 'element-plus';\r\n\r\nexport default {\r\n  name: 'RegisterView',\r\n  // const method = (rule, value, callback) => {\r\n    \r\n  // },\r\n  data() {\r\n    return {\r\n      userid: '',\r\n      username: '',\r\n      password: '',\r\n      phoneNum: '',\r\n      email: '',\r\n      campus: '',\r\n      address: '',\r\n      confirmPassword: '',\r\n      \r\n      step: 1,\r\n    };\r\n  },\r\n  methods: {\r\n    nextPage() {\r\n      let emailReg = /^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)+$/;\r\n      let passwordReg  = /^[.]{6,}$/;\r\n      if (this.username.length === 0 || this.password.length === 0 || this.email.length === 0 || this.confirmPassword.length === 0) {\r\n        ElMessage({message: \"输入不可为空\", type: 'warning'});\r\n      } else if (!emailReg.test(this.email)) {\r\n        // 如果不符合，显示错误信息\r\n        alert('请输入正确的邮箱地址');\r\n      } else if (!passwordReg.test(this.password)) {\r\n        ElMessage({message: \"密码长度需大于等于6\", type: 'warning'});\r\n      } else if (this.password !== this.confirmPassword) {\r\n        ElMessage({message: \"二次密码输入不一致\", type: 'warning'});\r\n      } else {\r\n        this.step = 2;\r\n      }\r\n    },\r\n    submitForm() {\r\n      this.$refs.registerForm.validate(valid => {\r\n        if (valid) {\r\n          // 执行注册逻辑，例如向后端发送注册请求\r\n          // 在注册成功后，可以进行页面跳转或其他操作\r\n          // 示例：向控制台打印注册信息\r\n          console.log('注册信息:', this.registerForm);\r\n          this.$router.push({ name: 'Main' });\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#body {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100vh;\r\n  overflow: hidden;\r\n  margin: 0;\r\n}\r\n\r\n#background {\r\n  background: url(\"../assets/image/backgroundOfLogin.jpg\");\r\n  background-size: contain;\r\n  position: absolute;\r\n  top: -50px;\r\n  bottom: -20px;\r\n  left: -20px;\r\n  right: -20px;\r\n  z-index: -1;\r\n}\r\n\r\n.form-wrapper {\r\n  width: 300px;\r\n  background-color: rgba(128, 128, 128, 0.8);\r\n  color: #fff;\r\n  border-radius: 20px;\r\n  padding: 50px;\r\n}\r\n\r\n.form-wrapper .header {\r\n  text-align: center;\r\n  font-size: 40px;\r\n  margin-bottom: 30px;\r\n  text-transform: uppercase;\r\n  line-height: 100px;\r\n}\r\n\r\n.form-wrapper .input-wrapper input,\r\n.form-wrapper .input-wrapper el-input {\r\n  background-color: rgb(255, 255, 255);\r\n  border: 0;\r\n  width: 100%;\r\n  text-align: center;\r\n  font-size: 15px;\r\n  color: rgb(96, 93, 9);\r\n  outline: none;\r\n}\r\n\r\n.form-wrapper .input-wrapper input::placeholder,\r\n.form-wrapper .input-wrapper el-input::placeholder {\r\n  text-transform: uppercase;\r\n}\r\n\r\n.form-wrapper .input-wrapper .border-wrapper {\r\n  background-image: linear-gradient(to right, #000000, #000000);\r\n  width: 100%;\r\n  height: 50px;\r\n  margin-bottom: 20px;\r\n  border-radius: 30px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.form-wrapper .input-wrapper .border-wrapper .border-item {\r\n  height: calc(100% - 4px);\r\n  width: calc(100% - 4px);\r\n  border-radius: 30px;\r\n}\r\n\r\n.form-wrapper .action {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.form-wrapper .action .btn {\r\n  width: 60%;\r\n  text-transform: uppercase;\r\n  border: 2px solid #3d3d3d;\r\n  text-align: center;\r\n  line-height: 50px;\r\n  border-radius: 30px;\r\n  cursor: pointer;\r\n}\r\n\r\n.form-wrapper .action .btn:hover {\r\n  background-color: rgba(79, 79, 79, 0.8);\r\n  color: #000000;\r\n}\r\n\r\n.form-wrapper .icon-wrapper {\r\n  text-align: center;\r\n  width: 60%;\r\n  margin: 0 auto;\r\n  margin-top: 20px;\r\n  border-top: 1px dashed rgb(146, 146, 146);\r\n  padding: 20px;\r\n}\r\n\r\n.el-message {\r\n  max-width: 300px;\r\n  position: fixed;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.form-wrapper .icon-wrapper i {\r\n  font-size: 20px;\r\n  color: rgb(187, 187, 187);\r\n  cursor: pointer;\r\n  border: 1px solid #fff;\r\n  padding: 5px;\r\n  border-radius: 20px;\r\n}\r\n\r\n.form-wrapper .icon-wrapper i:hover {\r\n  background-color: #0e92b3;\r\n}\r\n\r\n</style>\r\n"],"mappings":";AAsDA,SAASA,SAAQ,QAAS,cAAc;AAExC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpB;;EAEA;EACAC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,eAAe,EAAE,EAAE;MAEnBC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAIC,QAAO,GAAI,uCAAuC;MACtD,IAAIC,WAAU,GAAK,WAAW;MAC9B,IAAI,IAAI,CAACX,QAAQ,CAACY,MAAK,KAAM,KAAK,IAAI,CAACX,QAAQ,CAACW,MAAK,KAAM,KAAK,IAAI,CAACT,KAAK,CAACS,MAAK,KAAM,KAAK,IAAI,CAACN,eAAe,CAACM,MAAK,KAAM,CAAC,EAAE;QAC5HhB,SAAS,CAAC;UAACiB,OAAO,EAAE,QAAQ;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACjD,OAAO,IAAI,CAACJ,QAAQ,CAACK,IAAI,CAAC,IAAI,CAACZ,KAAK,CAAC,EAAE;QACrC;QACAa,KAAK,CAAC,YAAY,CAAC;MACrB,OAAO,IAAI,CAACL,WAAW,CAACI,IAAI,CAAC,IAAI,CAACd,QAAQ,CAAC,EAAE;QAC3CL,SAAS,CAAC;UAACiB,OAAO,EAAE,YAAY;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACrD,OAAO,IAAI,IAAI,CAACb,QAAO,KAAM,IAAI,CAACK,eAAe,EAAE;QACjDV,SAAS,CAAC;UAACiB,OAAO,EAAE,WAAW;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACpD,OAAO;QACL,IAAI,CAACP,IAAG,GAAI,CAAC;MACf;IACF,CAAC;IACDU,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,QAAQ,CAACC,KAAI,IAAK;QACxC,IAAIA,KAAK,EAAE;UACT;UACA;UACA;UACAC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAE,IAAI,CAACJ,YAAY,CAAC;UACvC,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC;YAAE5B,IAAI,EAAE;UAAO,CAAC,CAAC;QACrC;MACF,CAAC,CAAC;IACJ;EACF;AACF