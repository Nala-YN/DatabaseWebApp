{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nexport default {\n  name: 'RegisterView',\n  data() {\n    return {\n      // userid: 1,\n      username: '',\n      password: '',\n      phoneNum: '',\n      email: '',\n      campus: '',\n      address: '',\n      confirmPassword: '',\n      step: 1\n    };\n  },\n  methods: {\n    nextPage() {\n      let emailReg = /^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)+$/;\n      let passwordReg = /^(?=.*[A-Za-z])(?=.*\\d).{6,}$/;\n      if (this.username.length === 0 || this.password.length === 0 || this.email.length === 0 || this.confirmPassword.length === 0) {\n        ElMessage({\n          message: \"输入不可为空\",\n          type: 'warning'\n        });\n      } else if (!emailReg.test(this.email)) {\n        // 如果不符合，显示错误信息\n        // alert('请输入正确的邮箱地址');\n        ElMessage({\n          message: \"请输入正确的邮箱地址\",\n          type: 'warning'\n        });\n      } else if (!passwordReg.test(this.password)) {\n        ElMessage({\n          message: \"密码长度需大于等于6, 且数字和字母都要包含\",\n          type: 'warning'\n        });\n      } else if (this.password !== this.confirmPassword) {\n        ElMessage({\n          message: \"二次密码输入不一致\",\n          type: 'warning'\n        });\n      } else {\n        this.step = 2;\n      }\n    },\n    submitForm() {\n      let phoneReg = /^(?=.{10}\\d)$/;\n      if (this.phoneNum.length !== 0 && !phoneReg.test(this.phoneNum)) {\n        ElMessage({\n          message: \"请输入合法的电话号码\",\n          type: 'warning'\n        });\n      }\n      const registerForm = {\n        // userid: toString(this.userid++),\n        username: this.username,\n        password: this.password,\n        email: this.email,\n        phoneNum: this.phoneNum,\n        campus: this.campus,\n        address: this.address\n      };\n\n      // 发送注册请求到后端\n      // 假设使用axios库发送请求\n      axios.post('/api/register', registerForm).then(response => {\n        if (response.data.success) {\n          // 注册成功，执行登录逻辑，例如重定向到登录页面\n          alert('注册成功！');\n          this.$router.push('/login');\n        } else {\n          // 注册失败，处理失败情况，例如显示错误信息\n          // 已知错误类型：邮箱账户已存在\n          alert('注册失败：' + response.data.message);\n        }\n      }).catch(error => {\n        // 处理网络请求失败等情况\n        console.error('注册失败：', error);\n        alert('注册失败，请稍后重试');\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","name","data","username","password","phoneNum","email","campus","address","confirmPassword","step","methods","nextPage","emailReg","passwordReg","length","message","type","test","submitForm","phoneReg","registerForm","post","then","response","success","alert","$router","push","catch","error","console"],"sources":["E:\\Uni_homework\\DB\\test\\DatabaseWebApp\\client\\src\\views\\Register.vue"],"sourcesContent":["<template>\r\n    <!-- 整体背景 -->\r\n    <div id=\"body\">\r\n      <!-- <div id=\"background\"></div> -->\r\n      <!--输入框-->\r\n      <div class=\"form-wrapper\">\r\n        <div class=\"header\">创建书觅新账户</div>\r\n        <div v-if=\"step === 1\" class=\"input-wrapper\">\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"text\" name=\"username\" placeholder=\"username\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"username\" />\r\n          </div>\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"email\" name=\"email\" placeholder=\"email\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"email\" />\r\n          </div>\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"password\" name=\"password\" placeholder=\"password\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"password\" />\r\n          </div>\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"password\" name=\"confirm\" placeholder=\"confirm\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"confirmPassword\" />\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"input-wrapper\">\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"text\" name=\"phone\" placeholder=\"phone\" class=\"border-item\" autocomplete=\"off\"\r\n              pattern=\"[0-9]{10}\" v-model=\"phoneNum\" />\r\n          </div>\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"text\" name=\"campus\" placeholder=\"campus\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"campus\" />\r\n          </div>\r\n          <div class=\"border-wrapper\">\r\n            <input type=\"text\" name=\"address\" placeholder=\"address\" class=\"border-item\" autocomplete=\"off\"\r\n              v-model=\"address\" />\r\n          </div>\r\n        </div>\r\n  \r\n        <div v-if=\"step === 1\" class=\"action\">\r\n          <el-button class=\"btn\" :plain=\"true\" @click=\"$router.push('/login')\">登录</el-button>\r\n          <el-button class=\"btn\" :plain=\"true\" @click=\"nextPage\">下一步</el-button>\r\n        </div>\r\n        <div v-else class=\"action\">\r\n          <el-button class=\"btn\" :plain=\"true\" @click=\"step = 1\">上一步</el-button>\r\n          <el-button class=\"btn\" :plain=\"true\" @click=\"submitForm\">注册</el-button>\r\n        </div>\r\n        <br />\r\n      </div>\r\n    </div>\r\n    \r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  import { ElMessage } from 'element-plus';\r\n  \r\n  export default {\r\n    name: 'RegisterView',\r\n\r\n    data() {\r\n      return {\r\n        // userid: 1,\r\n        username: '',\r\n        password: '',\r\n        phoneNum: '',\r\n        email: '',\r\n        campus: '',\r\n        address: '',\r\n        confirmPassword: '',\r\n        \r\n        step: 1,\r\n      };\r\n    },\r\n    methods: {\r\n      nextPage() {\r\n        let emailReg = /^[\\w-]+(\\.[\\w-]+)*@[\\w-]+(\\.[\\w-]+)+$/;\r\n        let passwordReg  = /^(?=.*[A-Za-z])(?=.*\\d).{6,}$/;\r\n        if (this.username.length === 0 || this.password.length === 0 || this.email.length === 0 || this.confirmPassword.length === 0) {\r\n          ElMessage({message: \"输入不可为空\", type: 'warning'});\r\n        } else if (!emailReg.test(this.email)) {\r\n          // 如果不符合，显示错误信息\r\n          // alert('请输入正确的邮箱地址');\r\n          ElMessage({message: \"请输入正确的邮箱地址\", type: 'warning'});\r\n        } else if (!passwordReg.test(this.password)) {\r\n          ElMessage({message: \"密码长度需大于等于6, 且数字和字母都要包含\", type: 'warning'});\r\n        } else if (this.password !== this.confirmPassword) {\r\n          ElMessage({message: \"二次密码输入不一致\", type: 'warning'});\r\n        } else {\r\n          this.step = 2;\r\n        }\r\n      },\r\n      submitForm() {\r\n        let phoneReg = /^(?=.{10}\\d)$/;\r\n        if (this.phoneNum.length !== 0 && !phoneReg.test(this.phoneNum)) {\r\n          ElMessage({message: \"请输入合法的电话号码\", type: 'warning'});\r\n        }\r\n\r\n        const registerForm = {\r\n          // userid: toString(this.userid++),\r\n          username: this.username,\r\n          password: this.password,\r\n          email: this.email,\r\n          phoneNum: this.phoneNum,\r\n          campus: this.campus,\r\n          address: this.address\r\n        };\r\n  \r\n        // 发送注册请求到后端\r\n        // 假设使用axios库发送请求\r\n        axios\r\n          .post('/api/register', registerForm)\r\n          .then((response) => {\r\n            if (response.data.success) {\r\n              // 注册成功，执行登录逻辑，例如重定向到登录页面\r\n              alert('注册成功！');\r\n              this.$router.push('/login');\r\n            } else {\r\n              // 注册失败，处理失败情况，例如显示错误信息\r\n              // 已知错误类型：邮箱账户已存在\r\n              alert('注册失败：' + response.data.message);\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            // 处理网络请求失败等情况\r\n            console.error('注册失败：', error);\r\n            alert('注册失败，请稍后重试');\r\n          });\r\n  \r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  #body {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    height: 100vh;\r\n    overflow: hidden;\r\n    margin: 0;\r\n    border-collapse: collapse;\r\n\r\n    background: url(\"../assets/image/backgroundOfLogin.jpg\");\r\n    background-size: cover;\r\n    position: center top;\r\n    background-attachment: fixed;\r\n  }\r\n  \r\n  .form-wrapper {\r\n    width: 300px;\r\n    background-color: rgba(128, 128, 128, 0.8);\r\n    color: #fff;\r\n    border-radius: 20px;\r\n    padding: 50px;\r\n  }\r\n  \r\n  .form-wrapper .header {\r\n    text-align: center;\r\n    font-size: 40px;\r\n    margin-bottom: 30px;\r\n    text-transform: uppercase;\r\n    line-height: 100px;\r\n  }\r\n  \r\n  .form-wrapper .input-wrapper input,\r\n  .form-wrapper .input-wrapper el-input {\r\n    background-color: rgb(255, 255, 255);\r\n    border: 0;\r\n    width: 100%;\r\n    text-align: center;\r\n    font-size: 15px;\r\n    color: rgb(96, 93, 9);\r\n    outline: none;\r\n  }\r\n  \r\n  .form-wrapper .input-wrapper input::placeholder,\r\n  .form-wrapper .input-wrapper el-input::placeholder {\r\n    text-transform: uppercase;\r\n  }\r\n  \r\n  .form-wrapper .input-wrapper .border-wrapper {\r\n    background-image: linear-gradient(to right, #000000, #000000);\r\n    width: 100%;\r\n    height: 50px;\r\n    margin-bottom: 20px;\r\n    border-radius: 30px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .form-wrapper .input-wrapper .border-wrapper .border-item {\r\n    height: calc(100% - 4px);\r\n    width: calc(100% - 4px);\r\n    border-radius: 30px;\r\n  }\r\n  \r\n  .form-wrapper .action {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  \r\n  .form-wrapper .action .btn {\r\n    width: 60%;\r\n    text-transform: uppercase;\r\n    border: 2px solid #3d3d3d;\r\n    text-align: center;\r\n    line-height: 50px;\r\n    border-radius: 30px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .form-wrapper .action .btn:hover {\r\n    background-color: rgba(79, 79, 79, 0.8);\r\n    color: #000000;\r\n  }\r\n  \r\n  .form-wrapper .icon-wrapper {\r\n    text-align: center;\r\n    width: 60%;\r\n    margin: 0 auto;\r\n    margin-top: 20px;\r\n    border-top: 1px dashed rgb(146, 146, 146);\r\n    padding: 20px;\r\n  }\r\n  \r\n  .el-message {\r\n    max-width: 300px;\r\n    position: fixed;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n  }\r\n  \r\n  .form-wrapper .icon-wrapper i {\r\n    font-size: 20px;\r\n    color: rgb(187, 187, 187);\r\n    cursor: pointer;\r\n    border: 1px solid #fff;\r\n    padding: 5px;\r\n    border-radius: 20px;\r\n  }\r\n  \r\n  .form-wrapper .icon-wrapper i:hover {\r\n    background-color: #0e92b3;\r\n  }\r\n  \r\n  </style>\r\n  "],"mappings":";AAuDE,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,cAAc;AAExC,eAAe;EACbC,IAAI,EAAE,cAAc;EAEpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,eAAe,EAAE,EAAE;MAEnBC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAIC,QAAO,GAAI,uCAAuC;MACtD,IAAIC,WAAU,GAAK,+BAA+B;MAClD,IAAI,IAAI,CAACX,QAAQ,CAACY,MAAK,KAAM,KAAK,IAAI,CAACX,QAAQ,CAACW,MAAK,KAAM,KAAK,IAAI,CAACT,KAAK,CAACS,MAAK,KAAM,KAAK,IAAI,CAACN,eAAe,CAACM,MAAK,KAAM,CAAC,EAAE;QAC5Hf,SAAS,CAAC;UAACgB,OAAO,EAAE,QAAQ;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACjD,OAAO,IAAI,CAACJ,QAAQ,CAACK,IAAI,CAAC,IAAI,CAACZ,KAAK,CAAC,EAAE;QACrC;QACA;QACAN,SAAS,CAAC;UAACgB,OAAO,EAAE,YAAY;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACrD,OAAO,IAAI,CAACH,WAAW,CAACI,IAAI,CAAC,IAAI,CAACd,QAAQ,CAAC,EAAE;QAC3CJ,SAAS,CAAC;UAACgB,OAAO,EAAE,wBAAwB;UAAEC,IAAI,EAAE;QAAS,CAAC,CAAC;MACjE,OAAO,IAAI,IAAI,CAACb,QAAO,KAAM,IAAI,CAACK,eAAe,EAAE;QACjDT,SAAS,CAAC;UAACgB,OAAO,EAAE,WAAW;UAAEC,IAAI,EAAE;QAAS,C